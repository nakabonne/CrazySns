<p id="notice"><%= notice %></p>

<h1>ツイート一覧</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <p><td><%= link_to note.user.name, note.user%></td></p>
        <p><td><%= link_to note.content, note %></td></p>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
